{
  "files.associations": {
    "*.zmodel": "zmodel"
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "prettier.singleQuote": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "prettier.singleQuote": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[zmodel]": {
    "editor.tabSize": 2,
    "editor.formatOnPaste": true,
    "editor.defaultFormatter": "zenstack.zenstack"
  }
}